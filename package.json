{
  "name": "@mallzee/serverless-boilerplate",
  "version": "1.0.0",
  "description": "Mallzee Serverless boilerplate project",
  "main": "handler.js",
  "dependencies": {
    "aws-sdk": "^2.4.13",
    "jsonwebtoken": "^7.1.7"
  },
  "devDependencies": {
    "@mallzee/travis-notify-geckoboard": "^1.1.0",
    "aws-sdk": "^2.4.14",
    "aws-sdk-mock": "^1.4.1",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-config-mallzee": "^1.0.0",
    "ghooks": "^1.3.2",
    "githook": "0.0.1",
    "istanbul": "^0.4.4",
    "mocka": "0.0.1",
    "semantic-release": "^4.3.5",
    "serverless": "^1.0.0-beta.1.1",
    "sinon": "^1.17.5"
  },
  "scripts": {
    "geckoboard-start": "travis-notify-geckoboard -a $GECKOBOARD_API_KEY -w $GECKOBOARD_WIDGET_KEY -s",
    "geckoboard": "travis-notify-geckoboard -a $GECKOBOARD_API_KEY -w $GECKOBOARD_WIDGET_KEY",
    "lint": "eslint .",
    "test": "npm run lint && istanbul cover _mocha test/*.js -- -r test/common.js -R spec --recursive",
    "deploy": "npm test && sls deploy --stage $1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "mallzee",
    "serverless",
    "boilerplate"
  ],
  "author": "Mallzee",
  "license": "MIT"
}
